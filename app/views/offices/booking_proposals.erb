<h1> <%= @office.name %> </h1>

<% @booking_proposals.each do |temp_job| %>
  <hr/>

  <%=temp_job.time_range%>
  <h3> Proposals </h3>
  <ul>
    <% temp_job.requests.each do |proposal| %>
      <li>
        <% if proposal.sender_type == 'Professional' %>
          <%=proposal.sender.name%>
        <%else%>
          <%=proposal.receiver.name%>
        <%end%>
      </li>
    <% end %>
  </ul>
<%end%>
